    "@heroicons/react": "^2.1.5",
    "axios": "^1.7.7",
    "cargo": "^0.8.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@testing-library/jest-dom": "^6.6.3",
