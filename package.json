    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.20.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
